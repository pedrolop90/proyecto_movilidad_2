


  <% include partes/__cabecera %>
  <% include partes/__navegacion %>



  <title>Agregar Movilidad</title>
  <div class="content-wrapper">
    <section class="content container-fluid">
       <div class="row">
        <div class="col-xs-6 col-xs-push-3">
          <div class="box box-danger">
            <div class="box-header">
              <H1 class="box-title">AGREGAR TIPO MOVILIDAD</H1>
            </div>
              <form id="formulario_tipo_movilidad" method="post" onsubmit="return false;">
            <div class="box-body">
              <div class="form-group">
                <input type="text" name="nombre" placeholder="nombre" class="form-control" required>
              </div>
              <div class="form-group">
                <textarea  name="descripcion" placeholder="Descripcion" class="form-control" required></textarea>
              </div>
              <div class="col-lg-12">
               <h1 class="box-title">Requisito <label  id='agregarRequisito'
               class="btn btn-default" data-toggle="tooltip" data-placement="right"
               title="Agregar Requisito"><span class="glyphicon glyphicon-plus"></span>
                 </label></h1>
             </div>
             <div class="form-group" id='contenedorRequisitos'>

             </div>


          </div>
          <div class="box-footer">
            <button class="btn btn-primary center-block" id="botonEnviar">Agregar Movilidad</button>
          </div>
          </form>
        </div>
      </div>
    </div>
    </section>
  </div>
<script src="/js/agregarMovilidad.js"></script>
 <% include partes/__footer %>
